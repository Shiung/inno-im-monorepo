    "@sveltejs/kit": "latest",
    "autoprefixer": "^10.4.14",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.21",
    "rollup-plugin-svelte-svg": "1.0.0-beta.6",
    "svelte": "^3.54.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tailwind-config": "workspace:*",
    "tailwind-merge": "^1.10.0",
