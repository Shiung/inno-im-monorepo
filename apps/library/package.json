{
  "name": "im-library",
  "private": true,
  "version": "4.9.21",
  "type": "module",
  "scripts": {
    "dev": "npm run locales && npm run _dev",
    "dev:login": "LOGIN=true zx ./env_scripts/login.mjs",
    "build": "npm run locales && npm run _build",
    "build:library": "APP_ENV=library vite build",
    "build:library-watch": "APP_ENV=library vite build -w",
    "locales": "zx ./env_scripts/locales.mjs",
    "preview": "vite preview",
    "release": "zx ./env_scripts/releaseLibrary.mjs",
    "test": "vitest run .",
    "test:watch": "vitest .",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --check --ignore-path=../../.prettierignore . && eslint \"src\"",
    "format": "prettier --write --ignore-path=../../.prettierignore .",
    "_dev": "vite dev --port 5173 --open --host",
    "_build": "vite build",
    "dts-generate": "node ./env_scripts/generateDts.mjs"
  },
  "dependencies": {
    "@lottiefiles/svelte-lottie-player": "^0.3.0",
    "api": "workspace:*",
    "assets": "workspace:*",
    "bignumber.js": "^9.1.1",
    "d3": "^7.8.4",
    "env-config": "workspace:*",
    "localforage": "^1.10.0",
    "postcss-add-root-selector": "^1.0.5",
    "protobuf": "workspace:*",
    "svelte-portal": "^2.2.0",
    "svelte-spa-router": "^3.3.0",
    "tailwind-merge": "^1.10.0",
    "ui": "workspace:*",
    "utils": "workspace:*",
    "vd-resource": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/d3": "^7.4.0",
    "@types/node": "^18.15.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.28.0",
    "eslint-config-custom": "workspace:*",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^21.1.1",
    "node-env": "workspace:*",
    "node-fetch": "^3.3.1",
    "postcss": "^8.4.21",
    "sass": "^1.59.3",
    "svelte": "^3.55.1",
    "svelte-check": "^3.1.4",
    "svelte-preprocess": "^5.0.4",
    "svelte2tsx": "^0.6.12",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vite-plugin-svelte-svg": "^2.2.1",
    "vitest": "^0.29.7",
    "zx": "^7.2.1"
  }
}
