		"sass": "^1.59.3",
		"svelte": "^3.57.0",
		"svelte-check": "^2.9.2",
		"tailwind-config": "workspace:*",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.0.0",
		"vitest": "^0.25.3",
		"zx": "1.14.2"
