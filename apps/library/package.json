{
  "name": "im-library",
  "private": true,
  "version": "0.0.7",
  "type": "module",
  "scripts": {
    "dev": "npm run locales && npm run _dev",
    "_dev": "vite dev --port 5173 --open",
    "build": "npm run locales && npm run _build",
    "_build": "vite build",
    "build:library": "APP_ENV=library vite build",
    "locales": "zx ./env_scripts/locales.mjs",
    "preview": "vite preview",
    "release": "zx ./env_scripts/releaseLibrary.mjs",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --check --ignore-path=../../.prettierignore . && eslint \"src\"",
    "format": "prettier --write --ignore-path=../../.prettierignore ."
  },
  "dependencies": {
    "api": "workspace:*",
    "assets": "workspace:*",
    "env-config": "workspace:*",
    "localforage": "^1.10.0",
    "svelte-spa-router": "^3.3.0",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/node": "^18.15.6",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.28.0",
    "eslint-config-custom": "workspace:*",
    "eslint-plugin-svelte3": "^4.0.0",
    "node-env": "workspace:*",
    "node-fetch": "^3.3.1",
    "postcss": "^8.4.21",
    "sass": "^1.59.3",
    "svelte": "^3.55.1",
    "svelte-check": "^3.1.4",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vite-plugin-svelte-svg": "^2.2.1",
    "zx": "^7.2.1"
  }
}
